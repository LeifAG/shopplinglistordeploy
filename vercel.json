{
    "version": 2,
    "builds": [
        {
            "src": "shoppinglistor/wsgi.py",
            "use": "@vercel/python",
            "config": { 
                "maxLambdaSize": "15mb", 
                "runtime": "python3.9",
                "installCommand": "bash build_files.sh"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "shoppinglistor/wsgi.py"
        }
    ],
    "outputDirectory": "staticfiles"
}